import React from 'react'
import { Component } from 'react';


class Recieve extends Component{
    constructor(props){
        super(props);
        this.state = {
            id : props.match.params.id,
        }
    }

    render(){
        return(
            <div className="App">
                <header className="App-header">
                    <p>{this.state.id}</p>
                </header>
          </div>
        );
    }
}

export default Recieve;